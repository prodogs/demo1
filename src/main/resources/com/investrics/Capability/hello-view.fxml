<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="598.0" prefWidth="592.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.investrics.Capability.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TabPane prefHeight="353.0" prefWidth="552.0" tabClosingPolicy="ALL_TABS">
     <tabs>
       <Tab text="Untitled Tab 1" />
       <Tab text="Untitled Tab 2" />
     </tabs>
   </TabPane>
   <Label fx:id="titleText" />
   <ChoiceBox prefWidth="150.0" />
 <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
      <children>
       <AnchorPane layoutX="217.0" layoutY="-39.0" minHeight="0.0" minWidth="0.0" prefHeight="382.0" prefWidth="136.0">
            <children>
               <Button layoutX="22.0" layoutY="71.0" mnemonicParsing="false" onAction="#onGoodbyeClick" text="GoodBye" />
                <Button fx:id="myButton" layoutX="20.0" layoutY="112.0" onAction="#onHelloClick" text="Hello!" />
            </children>
         </AnchorPane>
      </children>
   </AnchorPane>

    <Label fx:id="welcomeText" />
</VBox>
